            </font>
        </Label>
        <ImageView fx:id="shieldImageView" fitHeight="73.0" fitWidth="59.0" layoutX="64.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/shield.png" />
        </ImageView>
    </AnchorPane>
    <Label layoutX="23.0" layoutY="126.0" prefHeight="17.0" prefWidth="69.0" text="Firstname" />
    <TextField fx:id="firstnameTextField" layoutX="20.0" layoutY="146.0" prefHeight="25.0" prefWidth="212.0" promptText="Firstname" />
    <Label layoutX="271.0" layoutY="124.0" prefHeight="17.0" prefWidth="69.0" text="Lastname" />
