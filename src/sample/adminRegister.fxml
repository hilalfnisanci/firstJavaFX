    <Button fx:id="adminRegisterButton" layoutX="276.0" layoutY="447.0" mnemonicParsing="false" onAction="#adminRegisterButtonOnAction" prefHeight="30.0" prefWidth="202.0" style="-fx-background-color: #169b29;" text="Register" textFill="#d0bebe">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </Button>
    <Button fx:id="cancelButton" layoutX="276.0" layoutY="486.0" mnemonicParsing="false" onAction="#adminCancelButtonOnAction" prefHeight="30.0" prefWidth="202.0" style="-fx-background-color: #169b29;" text="Cancel" textFill="#d0bebe">
