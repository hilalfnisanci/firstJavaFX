<AnchorPane prefHeight="546.0" prefWidth="520.0" style="-fx-background-color: #808080;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminRegisterController">
